<% if (errors.length > 0){ %>
    <div class="col my-3">
        <div class="alert alert-danger">
            <% errors.forEach(erro => { %>
                <%= erro %> <br>
            <%})%>
        </div>
    </div>
<% } %>

<% if (success.length > 0){ %>
    <div class="col my-3">
        <div class="alert alert-success">
            <% success.forEach(suc => { %>
                <%= suc %> <br>
            <%})%>
        </div>
    </div>
<% } %>



